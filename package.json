{
  "name": "@tepez/jasmine-protractor-matchers",
  "version": "2.1.0",
  "description": "Custom jasmine2 matchers for writing cleaner e2e tests with protractor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx gulp build",
    "test": "npx gulp build-unit-test",
    "test-debug": "npx --node-options=--inspect-brk gulp build-unit-test",
    "dev": "npx gulp dev-unit-test",
    "dev-debug": "npx --node-options=\"--inspect-brk\" gulp dev-unit-test",
    "update": "npx gulp build && npx np --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tepez/jasmine-protractor-matchers.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tepez/jasmine-protractor-matchers/issues"
  },
  "homepage": "https://github.com/tepez/jasmine-protractor-matchers#readme",
  "dependencies": {
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@tepez/typescript-build-gulp-tasks": "^6.5.1",
    "@tepez/typescript-common": "^0.2.2",
    "@types/gulp": "^4.0.9",
    "@types/jasmine": "^3.10.3",
    "@types/lodash.difference": "^4.5.6",
    "@types/lodash.intersection": "^4.4.6",
    "@types/node": "*",
    "@types/tinycolor2": "^1.4.3",
    "gulp": "^4.0.2",
    "jasmine": "^3.99.0",
    "np": "^7.6.0",
    "protractor": "^7.0.0"
  }
}
